{
  "expo": {
    "name": "QuizTracker",
    "slug": "quiz-tracker",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0d1012"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0d1012"
      },
      "package": "com.remonedev.quiztracker",
      "softwareKeyboardLayoutMode": "pan",
      "versionCode": 1,
      "androidStatusBar": {
        "translucent": false,
        "backgroundColor": "#111111",
        "barStyle": "light-content"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f6110326-4d91-472b-9daf-f1713a363e77"
      }
    },
    "plugins": [
      "expo-localization",
      "expo-font"
    ]
  }
}
